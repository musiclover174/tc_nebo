.about
  mt 100px
  &__row
    df()
    ml -15px
    mr -15px
    flex-wrap wrap
    @media $desktopMedium
      ml -10px
      mr -10px
    & + &
      mt 48px
      @media $desktopMedium
        mt 38px
  &__col
    padding 0 15px
    @media $desktopMedium
      padding 0 10px
    @media $LandscapeMedium
      flex 0 0 100%
      max-width 100%
      &_mapover
        dn()
    @media $LandscapeMediumMin
      &_col3
        flex 0 0 25%
        max-width 25%
      &_col4
        flex 0 0 33.333333%
        max-width 33.333333%
      &_col5
        flex 0 0 41.666667%
        max-width 41.666667%
      &_col7
        flex 0 0 58.333333%
        max-width 58.333333%
  &__text
    mt -8px
  &__place
    max-width 100%
  &__tomap
    max-width calc(100% - 15px)
  &__more
    c black
    border-bottom 1px solid $blue
    transition .7s border
    &:hover
      border-color transparent

.company
  &__about
    max-width 770px
    margin 0 auto
    &-time
      df(center, center)
      bgc $gblue
      mt 52px
      @media $PortraitMax
        padding 30px 0
      &-text
        c $blue
        fontify 24px 700 38px
    &-img
      margin -10px 53px -10px -65px
      @media $PortraitMax
        dn()
  &__we
    pos relative
    font-size 0
    mt 73px
    &-img
      max-width 100%
    &-title
      c white
      bgc $red
      pos absolute
      r 13%
      t 75%
      font-family $lf
      fontify 32px 700 38px
      padding 34px 33px 31px 39px
      max-width 413px
      @media $desktopMedium
        fontify 24px 700 36px
        padding 25px 31px 23px 33px
        max-width 447px
        t 83%
      @media $PortraitMax
        pos static
        l 0
        t 0
        max-width none
  &__text
    max-width 770px
    margin 103px auto 0
    @media $PortraitMax
      mt 60px
  &__col
    ib()
    w calc(50% - 17px)
    @media $PortraitMedium
      w 100%
    & + &
      ml 26px
      @media $PortraitMedium
        ml 0
        mt 25px
  &__file
    pos relative
    pl 129px
    mt 87px
    pt 20px
    @media $LandscapeMin
      pt 25px
      mt 60px
    @media $PortraitMedium
      pl 80px
      pt 15px
    &:before
      content ""
      pos absolute
      l 0
      t 0
      size 100px
      background url('data:image/png;base64,') no-repeat center $red
      @media $PortraitMedium
        size 60px
        background-size 80%
    &-download
      c black
      fontify 24px 800 30px
      border-bottom 1px solid $blue
      transition .7s border-color
      @media $desktopMedium
        fontify 21px 800 26px
      @media $PortraitMedium
        fontify 19px 800 24px
      &:hover
        border-color transparent
    &-info
      c black
      fontify 16px 400 26px
      mt 5px
  &__presentation
    pos relative
    h 550px
    mt 90px
    max-height 80vh
    background-size cover
    background-repeat no-repeat
    background-position center
    @media $LandscapeMedium
      h 450px
    @media $LandscapeMin
      h 400px
    &-href
      pos absolute
      t 50%
      l 50%
      size 55px
      bgc $red
      mt -27px
      ml -27px
      border-radius 50%
      &:before
        content ""
        pos absolute
        t 20px
        l 23px
        size 13px 16px
        background url('data:image/png;base64,') no-repeat center
    &-text
      pos absolute
      l calc(100% + 40px)
      c white
      fontify 24px 700 30px
      w 230px
      t 50%
      transform translateY(-50%)
      @media $LandscapeMin
        t calc(100% + 20px)
        l 50%
        w 280px
        text-align center
        transform translateX(-50%)
        br
          dn()
  &__chars
    max-width 770px
    margin 70px auto 0
    &-year
      max-width 100%
      &-wrapper
        pos relative
      &-value
        pos absolute
        t 25px
        l 38px
        c white
        font-size 70px
        font-weight bold
    &-list
      df()
      flex-wrap wrap
      mt 93px
      ml -30px
      w calc(100% + 30px)
      mb -60px
      @media $PortraitMedium
        mt 60px
    &-item
      df()
      ml 30px
      w calc(50% - 30px)
      mb 60px
      @media $LandscapeMin
        w calc(100% - 30px)
    &-imgover
      w 100px
      mr 30px
    &-info
      w calc(100% - 130px)
    &-digit
      c $blue
      fontify 70px 800 60px
      @media $LandscapeMedium
        fontify 50px 700 38px
    &-val
      c $blue
      fontify 21px 400 26px
      mt 6px
      @media $LandscapeMedium
        fontify 19px 400 24px
        mt 8px
  &__choice
    pos relative
    font-size 0
    mt 90px
    &-img
      max-width 100%
    &-title
      c white
      bgc $red
      pos absolute
      l 13%
      t 87%
      font-family $lf
      fontify 32px 700 38px
      padding 34px 33px 31px 39px
      max-width 413px
      @media $desktopMedium
        fontify 24px 700 36px
        padding 25px 31px 23px 33px
        max-width 357px
        t 85%
      @media $PortraitMax
        pos static
        l 0
        t 0
        max-width none
    @media $PortraitMaxFrom
      & + .clients
        mt 150px
  &__seo
    max-width 770px
    margin 70px auto 0
  &__feats
    mt 130px
    text-align center
    &-list
      df()
      flex-wrap wrap
      ml -30px
      w calc(100% + 30px)
      text-align left
      mb -55px
      @media $LandscapeMin
        mb -160px
      @media $PortraitMedium
        mb -100px
    &-item
      w calc(50% - 30px)
      ml 30px
      mb 55px
      bgc white
      box-shadow 0 3px 34.3px 0.7px rgba(112, 118, 126, 0.15)
      @media $LandscapeMin
        w calc(100% - 30px)
        mb 160px
      @media $PortraitMedium
        mb 100px
      @media $LandscapeMinFrom
        &:nth-child(2n)
          mt 90px
    &-imgover
      h 60px
      dfv(center, flex-end)
    &-img
      @media $PortraitMedium
        max-height 130px
    &-text
      c black
      fontify 16px 400 26px
      padding 18px 40px 30px 40px
    &-toall
      mt 40px
  &__gallery
    mt 70px
    padding 70px 0
    text-align center
    &-title
      mb 30px
    &-list
      df()
      flex-wrap wrap
      text-align left
      ml -15px
      mr -15px
      mb -40px
      @media $desktopMedium
        ml -10px
        mr -10px
      &_bottom
        align-items flex-end
        @media $LandscapeMedium
          align-items flex-start
      & + &
        mt 30px
        @media $desktopMedium
          mt 20px
    &-item
      mb 40px
      padding 0 15px
      @media $desktopMedium
        padding 0 10px
      @media $PortraitMedium
        flex 0 0 100%
        max-width 100%
        & + &
          mt 20px
      @media (max-width:950px) and (min-width:501px)
        flex 0 0 50%
        max-width 50%
      @media $LandscapeMediumMin
        flex 0 0 33.333333%
        max-width 33.333333%
    &-imgover
      db()
      w 100%
      pt 63%
      overflow hidden
      pos relative
      mb 23px
    &-img
      pos absolute
      l 0
      t 0
      size 100%
      object-fit cover
      object-position 100% 0
      font-family 'object-fit: cover;'
      transition 1.5s transform
      will-change transform
      -webkit-backface-visibility hidden
      transform scale(1.017) translateZ(0)
    &-imgover:hover &-img
      transform scale(1) translateZ(0)
    &-count
      pos absolute
      b 0
      l 0
      bgc $blue
      pl 46px
      pr 14px
      c white
      fontify 15px 400 37px
      &:before
        content ""
        pos absolute
        t 9px
        l 16px
        size 20px 17px
        background url('data:image/png;base64,') no-repeat center
    &-href
      c black
      fontify 21px 700 28px
      border-bottom 1px solid $blue
      transition .7s border, .7s color
      &:hover
        c $red
        border-color transparent
    &-imgover:hover + &-href
      c $red
      border-color transparent
    // &-more
    //   mt 50px