.tisers
  &__top
    df(flex-start, space-between)
    @media $LandscapeMedium
      flex-direction column
      justify-content flex-start
    &-text,
    &-side
      w calc(50% - 15px)
      @media $LandscapeMedium
        w 100%
    &-side
      mt 8px
      @media $LandscapeMedium
        mt 40px
    &-text
      pr 20px
      @media $LandscapeMedium
        pr 0
    @media $PortraitMediumFrom
      &-btn:after
        content ""
        display inline-block
        vertical-align middle
        ml 11px
        size 6px 8px
        background url('data:image/png;base64,') no-repeat center
  &__file
    pl 129px
    pt 5px
    pos relative
    @media $desktopMedium
      pt 10px
    @media $PortraitMedium
      pl 80px
    &:before
      content ""
      pos absolute
      l 0
      t 0
      size 100px
      background url('data:image/png;base64,') no-repeat center $red
      @media $PortraitMedium
        size 60px
        background-size 80%
    &-download
      c black
      fontify 24px 800 30px
      border-bottom 1px solid $blue
      transition .7s border-color
      @media $desktopMedium
        fontify 21px 800 26px
      @media $PortraitMedium
        fontify 19px 800 24px
      &:hover
        border-color transparent
    &-info
      c black
      fontify 16px 400 26px
      mt 5px
  &__coop
    df(center, space-between)
    mt 70px
    padding 70px 0
    @media $LandscapeMedium
      flex-direction column
      justify-content flex-start
      align-items flex-start
    &-left,
    &-right
      w calc(50% - 15px)
      @media $LandscapeMedium
        w 100%
    &-right
      df(center, flex-start)
      @media $LandscapeMedium
        mt 40px
      @media $PortraitMedium
        flex-direction column
        align-items flex-start
    &-title
      c $blue
      fontify 32px 800 38px
      mb 20px
      @media $PortraitMax
        fontify 24px 700 30px
    &-text
      max-width 410px
      @media $desktopMedium
        max-width 330px
      @media $LandscapeMedium
        max-width none
    &-imgover
      pos relative
      size 142px
      bgc white
      border-radius 50%
      oh()
      mr 30px
      @media $PortraitMedium
        mb 20px
    &-img
      pos absolute
      l 0
      b 0
      size 100% calc(100% - 15px)
      object-fit contain
      font-family 'object-fit: contain;'
    &-name
      c black
      fontify 16px 700 26px
      text-transform uppercase
      letter-spacing .02em
      mb 5px
    &-phones
      color black
      fontify 16px 400 26px
      mb 5px
    &-phone
      color black
    &-mail
      color black
      fontify 16px 400 26px
      border-bottom 1px solid $blue
      transition .7s border-color
      &:hover
        border-color transparent
  &__main
    mt 70px
    &-img
      max-width 100%
    &-title
      c $blue
      mt 60px
      fontify 44px 800 50px
      mb 30px
      @media $desktopMedium
        fontify 40px 700 44px
      @media $PortraitMax
        fontify 30px 700 34px
    &-variant
      & + &
        mt 35px
    &-imgover
      mt 40px
      mb 40px
      & > ^[0]__col:last-child
        dn()
  &__row
    df()
    flex-wrap wrap
    ml -15px
    mr -15px
    @media $desktopMedium
      ml -10px
      mr -10px
  &__col
    padding 0 15px
    @media $desktopMedium
      padding 0 10px
    @media $LandscapeMedium
      flex 0 0 100%
      max-width 100%
      & + &
        mt 20px
    @media $LandscapeMediumMin
      &_col4
        flex 0 0 33.333333%
        max-width 33.333333%
      &_col8
        flex 0 0 66.666667%
        max-width 66.666667%
  &__others
    mt 65px
    padding 50px 0
    &-title
      c $blue
      fontify 44px 800 50px
      mb 30px
      @media $desktopMedium
        fontify 40px 700 44px
      @media $PortraitMax
        fontify 30px 700 34px
  &__order
    mt 70px
    bgc white
    box-shadow 0 3px 43px rgba(0, 0, 0, 0.08)
    padding 50px 70px 70px
    @media $PortraitMax
      padding 0
      box-shadow none
      bgc transparent
    &-sub
      mt 40px
    &-hint
      mt 25px
    &-form
      mt 30px
      .fakecheckbox
        mt 27px
        @media $LandscapeMedium
          mt 16px