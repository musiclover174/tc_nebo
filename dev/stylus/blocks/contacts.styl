.contacts
  &:not(&_inner)
    mt 100px
  &__row
    df(stretch, flex-start)
    flex-wrap wrap
    ml -15px
    mr -15px
    &_personals
      mt 64px
      mb -66px
    @media $desktopMedium
      ml -10px
      mr -10px
    @media $LandscapeMedium
      flex-direction column-reverse
  &__col
    padding 0 15px
    @media $desktopMedium
      padding 0 10px
    @media $LandscapeMediumMin
      &_col4
        max-width 33.333333%
        flex 0 0 33.333333%
      &_col5
        pt 3px
        flex 0 0 41.666667%
        max-width 41.666667%
      &_col6
        flex 0 0 50%
        max-width 50%
      &_col7
        flex 0 0 58.333333%
        max-width 58.333333%
      &_col8
        flex 0 0 66.666667%
        max-width 66.666667%
  &__map
    w calc(100% - 30px)
    h 100%
    @media $desktopMedium
      w calc(100% - 20px)
    @media $LandscapeMedium
      w 100%
      h 400px
      max-height 60vh
  &__info
    pb 35px
    mt 28px
    @media (max-width:950px) and (min-width:501px)
      df()
      flex-wrap wrap
    &-row
      @media (max-width:950px) and (min-width:501px)
        w calc(50% - 10px)
        pr 15px
        & + &:nth-child(2)
          mt 0
      & + &
        mt 25px
    &-title
      fontify 21px 700 26px
      mb 6px
    &-text
      color black
      fontify 16px 400 26px
      & + &
        mt 5px
      &[href^="mailto"]
        border-bottom 1px solid $blue
        transition .7s border-color
        &:hover
          border-color transparent
  &__button
    mt 7px
    @media $LandscapeMinFrom
      padding 0 40px
  &__contact
    pos relative
    pl 59px
    pt 4px
    @media $LandscapeMedium
      mb 30px
    &:before
      content ""
      pos absolute
      l -2px
      t -2px
      size 40px
      border-radius 50%
      bgc $blue
      background-position center
      background-repeat no-repeat
    &[data-type="address"]:before
      background-image url('data:image/png;base64,')
    &[data-type="time"]:before
      background-image url('data:image/png;base64,')
    &[data-type="phone"]:before
      background-image url('data:image/png;base64,')
    &-title
      c $blue
      fontify 24px 700 26px
    &-text,
    &-phone
      c black
      fontify 16px 400 26px
    &-text
      mt 7px
  &__scheme
    pos relative
    mt 63px
    border 1px solid #e3e3e3
    h 620px
    @media $desktopMedium 
      h 510px
    @media $LandscapeMedium
      mt 53px
      h auto
      border none
    &-side
      w 270px
      padding 90px 50px 40px
      @media $LandscapeMedium
        w 100%
        padding 0
        mb 40px
      .common__title
        mb 20px
    &-map
      size 100%
      &-wrapper
        pos absolute
        r -1px
        t -1px
        b -1px
        l 270px
        oh()
        @media $LandscapeMedium
          pos static
          h 400px
          max-height 60vh
    &-href
      c black
      fontify 16px 400 26px
      border-bottom 1px solid $blue
      transition .7s border-color
      &:hover
        border-color transparent
  &__personal
    &-top
      background url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAAAAAA6fptVAAAABGdBTUEAALGPC/xhBQAAACBjSFJNAAB6JgAAgIQAAPoAAACA6AAAdTAAAOpgAAA6mAAAF3CculE8AAAAAmJLR0QA/4ePzL8AAAAJcEhZcwAACxIAAAsSAdLdfvwAAAAHdElNRQfjAhQLMjA+Q9sYAAAACklEQVQI12N4DAAA5QDkvR9+MwAAAABJRU5ErkJggg==') repeat-x right bottom 14px
      @media $desktopMedium
        mr 20px
        background-position right bottom 12px
      @media $LandscapeMedium
        mr 0
      @media $PortraitMedium
        background-position right bottom 11px
    &-position
      c $blue
      fontify 24px 700 30px
      bgc white
      pr 20px
      @media $desktopMedium
        fontify 21px 800 26px
      @media $PortraitMedium
        fontify 19px 800 24px
    &-main
      df(center, flex-start)
      mt 20px
      mb 66px
      @media $desktopMedium
        mr 20px
      @media $LandscapeMedium
        mr 0
      @media $PortraitMedium
        flex-direction column
        align-items flex-start
    &-title
      c $blue
      fontify 32px 800 38px
      mb 20px
      @media $PortraitMax
        fontify 24px 700 30px
    &-text
      max-width 410px
      @media $desktopMedium
        max-width 330px
      @media $LandscapeMedium
        max-width none
    &-imgover
      pos relative
      size 142px
      bgc #ebe9eb
      border-radius 50%
      oh()
      mr 30px
      flex-shrink 0
      @media $PortraitMedium
        mb 20px
    &-img
      pos absolute
      l 0
      b 0
      size 100% calc(100% - 15px)
      object-fit contain
      font-family 'object-fit: contain;'
    &-name
      c black
      fontify 16px 700 26px
      text-transform uppercase
      letter-spacing .02em
      mb 5px
    &-phones
      color black
      fontify 16px 400 26px
      mb 5px
    &-phone
      color black
    &-mail
      color black
      fontify 16px 400 26px
      border-bottom 1px solid $blue
      transition .7s border-color
      &:hover
        border-color transparent
  &__company
    pos relative
    df(center, space-between)
    mt 70px
    bgc white
    box-shadow 0 3px 43px rgba(0, 0, 0, 0.08)
    padding 72px
    @media $desktopMedium
      padding 50px
    @media $LandscapeMedium
      flex-direction column
      align-items flex-start
      justify-content flex-start
    @media $PortraitMax
      padding 0
      box-shadow none
      bgc transparent
    &:before
      content ""
      pos absolute
      bgc #e3e3e3
      width 1px
      l 45%
      t 72px
      b 72px
      @media $desktopMedium
        l 42%
        t 50px
        b 50px
      @media $LandscapeMedium
        dn()
    &-imgover
      w 42%
      text-align center
      @media $desktopMedium
        w 38%
      @media $LandscapeMedium
        w 100%
        mb 30px
        text-align left
    &-img
      max-width 90%
    &-about
      width 48.5%
      @media $desktopMedium
        w 52%
      @media $LandscapeMedium
        w 100%
    &-name
      c $blue
      fontify 24px 700 30px
      mb 25px
      @media $desktopMedium
        fontify 21px 800 26px
      @media $PortraitMedium
        fontify 19px 800 24px
    &-address
      c black
      fontify 16px 400 26px
    &-href
      c black
      fontify 16px 400 26px
      border-bottom 1px solid $blue
      transition .7s border-color
      &:hover
        border-color transparent

.map
  &__wrapper
    pos relative
    mr 0
    ml auto
    // max-width 985px
    max-width 1280px
  &__floors
    pos absolute
    z-index 1
    dfv()
    r 0
    t 50%
    transform translateY(-50%)
    &-btn
      pos relative
      size 60px
      bgc $blue
      c white
      fontify 16px 800 60px
      transition .7s background-color
      @media $LandscapeMin
        size 40px
        fontify 14px 400 40px
      &:not(:last-child):after
        content ""
        pos absolute
        b 0
        l 10px
        size 40px 1px
        bgc $dblue
        transition .7s background
        @media $LandscapeMin
          w 26px
          l 7px
      &.active,
      &:hover
        bgc $red
        &:after
          bgc transparent
  &__floor
    pos relative
    df(flex-end, flex-start)
    &:not(.active)
      dn()
    &-img
      max-width calc(100% - 83px)
      @media $LandscapeMin
        max-width calc(100% - 53px)
    &-title
      pos absolute
      l 0
      // t 51px
      t 0
      @media $PortraitMax
        dn()
      &:before
        white-space nowrap
  &__brief
    mt 64px
    &-list
      df()
      flex-wrap wrap
      ml -50px
      w calc(100% + 50px)
      mt 30px
    &-item
      pos relative
      ml 50px
      mb 30px
      pl 67px
      fontify 16px 400 25px
      pt 12px
      &-iconover
        dfv(center, center)
        pos absolute
        l 0
        t 0
        size 48px
        bgc #0066b3
        border-radius 50%
      &-icon
        max-width 90%
        max-height 90%
  
